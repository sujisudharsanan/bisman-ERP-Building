# AI service
# Base URL of your AI API
AI_BASE_URL=https://YOUR_AI_SERVICE.up.railway.app
AI_KEY=
AI_DEFAULT_MODEL=llama3


# Tawk inline chat
NEXT_PUBLIC_TAWK_ENABLED=true
